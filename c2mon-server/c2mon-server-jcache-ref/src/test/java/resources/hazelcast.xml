<hazelcast xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.hazelcast.com/schema/config
                               http://www.hazelcast.com/schema/config/hazelcast-config-3.8.xsd"
           xmlns="http://www.hazelcast.com/schema/config">

  <network>
    <join>
      <tcp-ip enabled="true">
        <interface>127.0.0.1</interface>
      </tcp-ip>
      <multicast enabled="false"/>
    </join>
  </network>

  <group>
    <name>test</name>
    <password>test</password>
  </group>

  <cache name="commandTagCache">
    <key-type class-name="java.lang.Long"/>
    <value-type class-name="cern.c2mon.shared.common.command.CommandTag"/>
  </cache>
  <cache name="dataTagCache">
    <key-type class-name="java.lang.Long"/>
    <value-type class-name="cern.c2mon.server.common.datatag.DataTag"/>
  </cache>
</hazelcast>